@using GoodeBooks.Services.ViewModels.Volumes;

@model VolumeCreateViewModel

<form asp-action="CreateNewVolume" asp-controller="Volume" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Kind"></label>
        <input asp-for="Kind" class="form-control" />
        <span asp-validation-for="Kind" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Etag"></label>
        <input asp-for="Etag" class="form-control" />
        <span asp-validation-for="Etag" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="VolumeInfoId"></label>
        <input asp-for="VolumeInfoId" class="form-control" />
        <span asp-validation-for="VolumeInfoId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="SaleInfoId"></label>
        <input asp-for="SaleInfoId" class="form-control" />
        <span asp-validation-for="SaleInfoId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="SearchInfoId"></label>
        <input asp-for="SearchInfoId" class="form-control" />
        <span asp-validation-for="SearchInfoId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="BookshelfIds"></label>
        <input asp-for="BookshelfIds" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

